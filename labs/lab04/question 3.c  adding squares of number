#include <stdio.h>

int main ()  {
	int n=0;	
	int sum=0;	
	int count=0;
	int number=0;
        
	printf("input n");
	scanf("%d", &n);
	while(count<n) {
	printf("enter a number");
	scanf("%d", &number);
	 count++;
	number= number*number;
	sum=sum+number;
	

}

	printf("your sum is: %d", sum);



	return 0;
}
